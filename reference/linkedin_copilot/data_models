//Data Models
//LinkedIn Session
interface LinkedInSession {
  id: number;
  created_at: string;
  session_id: string;
  last_activity: string | null;
  is_enabled: boolean;
  user_id: number;
  session_name: string;
  is_deleted: boolean;
  linkedin_campaigns_id: number;
}
//LinkedIn Message
interface LinkedInMessage {
  id: number;
  created_at: string;
  session_id: string;
  role: 'user' | 'ai';
  content: string;
  user_id: number;
}
//LinkedIn Campaign
interface LinkedInCampaign {
  id: number;
  created_at: string;
  name: string;
  additional_notes: string;
  company_id: number;
  created_by: number;
  marketing_type: string;
  post_length: number;
  tone: string;
}
//Campaign Page Structure
interface CampaignPage {
  linkedin_campaigns_id: number;
  name: string;
  records: LinkedInSession[];
}
